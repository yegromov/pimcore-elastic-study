[{
    "name":"Message Queue",
    "comment":"this command fills the message queue with the necessary tasks, which are then processed by messenger:consume",
    "schedule":"@every 5m",
    "project":"php-beauty",
    "container":"php-beauty_php-fpm",
    "command":"./bin/console pimcore:maintenance --async",
    "onstart":true
 }
,{
    "name":"Maintenance",
    "comment":"it's recommended to run the following command using a process control system like Supervisor",
    "schedule":"@every 5m",
    "project":"php-beauty",
    "container":"php-beauty_php-fpm",
    "command":"./bin/console messenger:consume pimcore_core pimcore_maintenance --time-limit=300",
    "onstart":true
},
,{
    "name":"Parallel import",
    "comment":"Process import queue items that can be executed in parallel",
    "schedule":"@every 5m",
    "project":"php-beauty",
    "container":"php-beauty_php-fpm",
    "command":"./bin/console datahub:data-importer:process-queue-parallel --processes=5",
    "onstart":true
},
,{
    "name":"Sequental import",
    "comment":"Process import queue items that need to be executed sequentially ",
    "schedule":"@every 5m",
    "project":"php-beauty",
    "container":"php-beauty_php-fpm",
    "command":"./bin/console datahub:data-importer:process-queue-sequential",
    "onstart":true
},
,{
    "name":"Import status cron check",
    "comment":"Import status cron check",
    "schedule":"@every 1m",
    "project":"php-beauty",
    "container":"php-beauty_php-fpm",
    "command":"./bin/console datahub:data-importer:execute-cron",
    "onstart":true
},
]







